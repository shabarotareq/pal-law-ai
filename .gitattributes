*.bin filter=lfs diff=lfs merge=lfs -text
*.zip filter=lfs diff=lfs merge=lfs -text
*.rar filter=lfs diff=lfs merge=lfs -text
# تعطيل تحويل نهايات الأسطر لملفات package.json
package.json text eol=lf
package-lock.json text eol=lf
*.js text eol=lf
*.jsx text eol=lf
*.ts text eol=lf
*.tsx text eol=lf
*.html text eol=lf
*.css text eol=lf
*.json text eol=lf
3dmodels/*.zip filter=lfs diff=lfs merge=lfs -text
